    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "husky": "^9.1.7",
    "prettier": "^2.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
